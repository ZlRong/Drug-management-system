<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.bean">
	<class name="UserLogin" table="user_login">
		<id name="id" column="id" type="long">
			<generator class="identity"></generator>
		</id>
		<property name="number" column="number" type="string" length="100" />
		<property name="username" column="username" type="string" length="30" />
		<property name="password" column="password" type="string" length="30" />
	</class>
</hibernate-mapping>