<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.bean">
	<class name="MedicineStorehouse" table="medicine_storehouse">
		<id name="id" column="id" type="long">
			<generator class="identity"></generator>
		</id>
		<property name="medicineId" column="medicine_id" type="long" />
		<property name="mfg" column="mfg" type="date" />
		<property name="exp" column="exp" type="date" />
		<property name="number" column="number" type="int" />
		<property name="price" column="price" type="double" />
		<property name="purchaseId" column="purchase_id" type="long" />
	</class>
</hibernate-mapping>